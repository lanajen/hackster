= link_to 'Facebook', user_omniauth_authorize_url(:facebook, host: APP_CONFIG['default_host'], redirect_host: request.host, redirect_to: @redirect_to), class: 'zocial zocial-block facebook', tabindex: (defined?(autofocus) and autofocus ? 1 : nil)
= link_to 'Github', user_omniauth_authorize_url(:github, host: APP_CONFIG['default_host'], redirect_host: request.host, redirect_to: @redirect_to), class: 'zocial zocial-block github'
= link_to 'Google+', user_omniauth_authorize_url(:gplus, host: APP_CONFIG['default_host'], redirect_host: request.host, redirect_to: @redirect_to), class: 'zocial zocial-block googleplus'
/ = link_to 'LinkedIn', user_omniauth_authorize_path(:linkedin), class: 'zocial zocial-block linkedin'
= link_to 'Twitter', user_omniauth_authorize_url(:twitter, host: APP_CONFIG['default_host'], redirect_host: request.host, redirect_to: @redirect_to), class: 'zocial zocial-block twitter'
