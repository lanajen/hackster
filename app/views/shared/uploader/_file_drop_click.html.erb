$(function(){
  $('.file-drop<%= ".#{file_type}" if defined?(file_type) %>').on('click', 'a.browse', function(e){
    e.preventDefault();
    $($(this).data('target')).find('input[type=file]').click();
  });
  $('.file-drop<%= ".#{file_type}" if defined?(file_type) %>').on('click', 'a.url', function(e){
    e.preventDefault();
    $($(this).data('target')).show();
  });
});