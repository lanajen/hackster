- if url
  - case type
  - when :facebook_link
    = link_to content_tag(:i, '', class: 'icon-facebook-sign'), url
  - when :linked_in_link
    = link_to content_tag(:i, '', class: 'icon-linkedin-sign'), url
  - when :twitter_link
    = link_to content_tag(:i, '', class: 'icon-twitter-sign'), url
  - when :website_link
    = link_to content_tag(:i, '', class: 'icon-link'), url
  - when :blog_link
    = link_to content_tag(:i, '', class: 'icon-rss'), url
  - when :github_link
    = link_to content_tag(:i, '', class: 'icon-github-sign'), url
